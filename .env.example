# Copy this file to .env and fill in your keys.
BINANCE_API_KEY=your_key_here
BINANCE_API_SECRET=your_secret_here

# true -> testnet, false -> live
TESTNET=true

# true -> simulate orders without sending to exchange
PAPER_TRADING=true

# Trading parameters
BASE_QUOTE=USDT
UNIVERSE_SIZE=12
MAX_OPEN_POSITIONS=5
TIMEFRAME=5m
RISK_PER_TRADE=0.01        # 1% of equity
STOP_LOSS_PCT=0.03         # 3% below entry
TAKE_PROFIT_PCT=0.06       # 6% above entry
MIN_RSI=55
MAX_RSI=80
LOOKBACK_BARS=50
COOLDOWN_MINUTES=60
MIN_24H_QUOTE_VOLUME=20000000  # $20m

# Optional: corporate proxy (if needed for network)
# HTTP_PROXY=http://127.0.0.1:8888
# HTTPS_PROXY=http://127.0.0.1:8888
